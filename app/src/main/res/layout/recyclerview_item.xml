<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp113"
    android:layout_marginTop="@dimen/dp10"
    android:background="#FFFFFF">

    <TextView
        android:id="@+id/tv_time_content"
        style="@style/Text_12gray_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp12"
        android:layout_marginTop="@dimen/dp4"
        android:gravity="center"
        android:text=""
        android:layout_marginLeft="@dimen/dp12" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp1"
        android:layout_marginTop="@dimen/dp29"
        android:background="#F7F7F7" />

    <ImageView
        android:id="@+id/icon"
        android:layout_width="@dimen/dp16"
        android:layout_height="@dimen/dp16"
        android:layout_marginLeft="@dimen/dp15"
        android:layout_marginTop="@dimen/dp38"
        android:scaleType="centerCrop" />

    <TextView
        android:id="@+id/state_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp6"
        android:layout_marginTop="@dimen/dp38"
        android:layout_toRightOf="@+id/icon"
        android:text=""
        android:textColor="#FC8968"
        android:textSize="@dimen/sp12" />

    <TextView
        android:id="@+id/tv_from_or_send"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp12"
        android:layout_marginTop="@dimen/dp63"
        android:paddingBottom="@dimen/dp2.5"
        android:paddingTop="@dimen/dp2.5"
        android:text=""
        android:textColor="#999999"
        android:textSize="@dimen/sp12" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp12"
        android:layout_marginTop="@dimen/dp83"
        android:paddingBottom="@dimen/dp2.5"
        android:paddingTop="@dimen/dp2.5"
        android:text="@string/status"
        android:textColor="#ABAFB4"
        android:textSize="@dimen/sp12"
        android:id="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/dp12"
        android:layout_marginTop="@dimen/dp38"
        android:paddingBottom="@dimen/dp2.5"
        android:paddingTop="@dimen/dp2.5"
        android:id="@+id/tv_send_or_receive"
        android:text=""
        android:textColor="#FC8968"
        android:textSize="@dimen/sp12" />

    <TextView
        android:id="@+id/id_item_list_title"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/dp12"
        android:layout_marginTop="@dimen/dp63"
        android:paddingBottom="@dimen/dp2.5"
        android:paddingTop="@dimen/dp2.5"
        android:singleLine="true"
        android:gravity="center|right"
        android:layout_width="@dimen/dp196"
        android:ellipsize="middle"
        android:text=""
        android:textColor="#333333"
        android:textSize="@dimen/sp12" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView3"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true">

        <TextView
            android:id="@+id/tv_confirm_count"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp12"
            android:layout_marginTop="@dimen/dp85"
            android:paddingBottom="@dimen/dp2.5"
            android:paddingTop="@dimen/dp1"
            android:text=""
            android:textColor="#333333"
            android:textSize="@dimen/sp12" />

        <TextView
            android:id="@+id/tv_zhuan_catch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp12"
            android:layout_marginTop="@dimen/dp85"
            android:paddingBottom="@dimen/dp2.5"
            android:paddingTop="@dimen/dp2.5"
            android:text="@string/tv_zhuan_catch"
            android:textColor="@color/red_color"
            android:textSize="@dimen/sp12"
            android:visibility="gone" />

    </RelativeLayout>

    <!--<View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp4"
        android:layout_alignParentBottom="true"
        android:background="#e3e3e3"/>-->
</RelativeLayout>
